{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es6",
      "outDir": "dist",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": ["es6"],
      "skipLibCheck": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "declaration": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"],
  }
  